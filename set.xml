<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 	xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-4.0.xsd">
    
    <bean id="bird" class="com.spring.set.Bird">
    <property name="eagle">
    <set>
    <ref bean="eaglea"/>
    <ref bean="eagleb"/>
    <ref bean="eaglec"/>
    </set>
    </property>
    </bean> 
    <bean id="eaglea" class="com.spring.set.Eagle">
    <property name="colour" value="black"/>
    </bean>
    <bean id="eagleb" class="com.spring.set.Eagle">
    <property name="colour" value="white"/>
    </bean>
     <bean id="eaglec" class="com.spring.set.Eagle">
    <property name="colour" value="red"/>
    </bean>
    </beans>
